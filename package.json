{
  "name": "metal-soy-bundle",
  "version": "1.1.5",
  "description": "A bundle containing all the closure dependencies required by soy files compiled to incremental-dom",
  "license": "MIT",
  "repository": "metal/metal-soy-bundle",
  "author": {
    "name": "Maira Bello",
    "email": "maira.araujo@liferay.com"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "jsnext:main": "build/bundle.js",
  "main": "lib/bundle.js",
  "files": [
    "build",
    "lib"
  ],
  "scripts": {
    "build": "gulp",
    "compile": "babel --presets es2015 -d lib/bundle.js build/bundle.js",
    "lint": "jshint gulpfile.js",
    "prepublish": "npm run build && npm run compile"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "closure-templates": "^20151008.0.0",
    "google-closure-library": "^20160218.0.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.7.1",
    "gulp-replace": "^0.5.4",
    "jshint": "^2.7.0"
  },
  "dependencies": {
    "metal-incremental-dom": "^1.0.0-rc.2"
  }
}
